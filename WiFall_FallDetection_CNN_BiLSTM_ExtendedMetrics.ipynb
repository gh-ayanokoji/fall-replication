</span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"Precision    : <span class="hljs-subst">{prec:<span class="hljs-number">.2</span></span></span></span><span>f}%")
</span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"Recall (TPR) : <span class="hljs-subst">{rec:<span class="hljs-number">.2</span></span></span></span><span>f}%")
</span><span><span class="hljs-built_in">print</span></span><span>(</span><span><span class="hljs-string">f"Specificity  : <span class="hljs-subst">{specificity:<span class="hljs-number">.2</span></span></span></span><span>f}%")
