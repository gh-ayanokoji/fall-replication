    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__init__</span></span><span>(</span><span><span class="hljs-params">self, root_dir</span></span><span>):
        self.samples, self.labels = [], []
        self.label_map = {</span><span><span class="hljs-string">"no_fall"</span></span><span>: </span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-string">"fall"</span></span><span>: </span><span><span class="hljs-number">1</span></span><span>}
        </span><span><span class="hljs-keyword">for</span></span><span> label </span><span><span class="hljs-keyword">in</span></span><span> [</span><span><span class="hljs-string">"no_fall"</span></span><span>, </span><span><span class="hljs-string">"fall"</span></span><span>]:
            folder = os.path.join(root_dir, label)
            </span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> os.listdir(folder):
                </span><span><span class="hljs-keyword">if</span></span><span> f.endswith(</span><span><span class="hljs-string">".npy"</span></span><span>):
                    self.samples.append(os.path.join(folder, f))
                    self.labels.append(self.label_map[label])

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__len__</span></span><span>(</span><span><span class="hljs-params">self</span></span><span>): </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-built_in">len</span></span><span>(self.samples)

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__getitem__</span></span><span>(</span><span><span class="hljs-params">self, idx</span></span><span>):
