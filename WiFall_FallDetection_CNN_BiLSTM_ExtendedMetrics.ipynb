    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__init__</span></span><span>(</span><span><span class="hljs-params">self, root_dir, transform=<span class="hljs-literal">None</span></span></span><span>):
        self.samples = []
        self.labels = []
        self.transform = transform
        self.label_map = {</span><span><span class="hljs-string">"no_fall"</span></span><span>:</span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-string">"fall"</span></span><span>:</span><span><span class="hljs-number">1</span></span><span>}
        </span><span><span class="hljs-keyword">for</span></span><span> label </span><span><span class="hljs-keyword">in</span></span><span> [</span><span><span class="hljs-string">"no_fall"</span></span><span>,</span><span><span class="hljs-string">"fall"</span></span><span>]:
            path = os.path.join(root_dir, label)
            </span><span><span class="hljs-keyword">for</span></span><span> f </span><span><span class="hljs-keyword">in</span></span><span> os.listdir(path):
                </span><span><span class="hljs-keyword">if</span></span><span> f.endswith(</span><span><span class="hljs-string">".npy"</span></span><span>):
                    self.samples.append(os.path.join(path,f))
                    self.labels.append(self.label_map[label])

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__len__</span></span><span>(</span><span><span class="hljs-params">self</span></span><span>):
        </span><span><span class="hljs-keyword">return</span></span><span> </span><span><span class="hljs-built_in">len</span></span><span>(self.samples)

    </span><span><span class="hljs-keyword">def</span></span><span> </span><span><span class="hljs-title function_">__getitem__</span></span><span>(</span><span><span class="hljs-params">self, idx</span></span><span>):
        data = np.load(self.samples[idx])  </span><span><span class="hljs-comment"># shape: (subcarriers, timesteps)</span></span><span>
        </span><span><span class="hljs-comment"># convert to spectrogram or time-freq representation</span></span><span>
        S = np.</span><span><span class="hljs-built_in">abs</span></span><span>(librosa.stft(data.mean(axis=</span><span><span class="hljs-number">0</span></span><span>), n_fft=</span><span><span class="hljs-number">128</span></span><span>, hop_length=</span><span><span class="hljs-number">64</span></span><span>))
        S = (S - S.mean()) / (S.std() + </span><span><span class="hljs-number">1e-6</span></span><span>)
        S = np.expand_dims(S, axis=</span><span><span class="hljs-number">0</span></span><span>).astype(np.float32)  </span><span><span class="hljs-comment"># shape (1, H, W)</span></span><span>
        label = self.labels[idx]
        </span><span><span class="hljs-keyword">return</span></span><span> torch.tensor(S), torch.tensor(label, dtype=torch.long)

</span><span><span class="hljs-comment"># Prepare loaders</span></span><span>
