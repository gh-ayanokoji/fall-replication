        </span><span><span class="hljs-keyword">if</span></span><span> data.ndim == </span><span><span class="hljs-number">1</span></span><span>: data = np.expand_dims(data, axis=</span><span><span class="hljs-number">0</span></span><span>)
        </span><span><span class="hljs-keyword">if</span></span><span> data.ndim == </span><span><span class="hljs-number">2</span></span><span> </span><span><span class="hljs-keyword">and</span></span><span> data.shape[</span><span><span class="hljs-number">0</span></span><span>] &gt; data.shape[</span><span><span class="hljs-number">1</span></span><span>]: data = data.T
